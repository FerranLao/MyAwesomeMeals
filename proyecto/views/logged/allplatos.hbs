<h1>Your Recipes</h1>
{{#each recipes}}
<div class="recipe">
    <p>{{this.name}}</p>
    <img src="{{this.imgPath}}" height="200px">
    <a href="/logged/recipe/{{this._id}}">see this recipe in detail</a>
    <button class="recipeRemove">Remove this recipe</button>
</div>
{{/each}}
<script src="../javascripts/allplatos.js"></script>
